@page
@model Friberg_car_rentals_v2.Pages.Login.LoginModel
@{
    ViewData["Title"] = "Login";
}

<h2>@ViewData["Title"]</h2>

<form method="post">
    <div>
        <label>Username:</label>
        <input name="Username" />
    </div>

    <div>
        <label>Password:</label>
        <input type="password" name="Password" />
    </div>

    <div>
        <button type="submit">Log in</button>
    </div>
</form>
