@page "/logout"
@model Friberg_car_rentals_v2.Pages.Login.LogoutModel
@{
    ViewData["Title"] = "Logga ut";
}

<h2>@ViewData["Title"]</h2>
<div>
   
        @if (ViewData["CurrentUserName"] != null)
        {
            <p>
                Logga ut som @ViewData["CurrentUserName"]
            </p>
        }
        else 
        {
            <p>
                Logga ut som @ViewData["CurrentAdmin"]
            </p>

        }
    
</div>
<div>
    <form method="post" asp-page="/Logout">
        <button type="submit">Logga ut</button>
    </form>
</div>
